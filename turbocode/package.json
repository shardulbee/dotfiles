{
  "name": "turbocode",
  "displayName": "TurboCode",
  "description": "Custom productivity utilities for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "turbocode.github.openPermalink",
        "title": "TurboCode: Open GitHub Permalink"
      },
      {
        "command": "turbocode.github.openPermalinkOnMain",
        "title": "TurboCode: Open GitHub Permalink on Main"
      },
      {
        "command": "jj.log.open",
        "title": "JJ: Open Log View"
      },
      {
        "command": "jj.log.toggleExpand",
        "title": "JJ: Toggle Expand Commit"
      },
      {
        "command": "jj.log.edit",
        "title": "JJ: Edit/Checkout Commit"
      },
      {
        "command": "jj.log.describe",
        "title": "JJ: Describe Commit"
      },
      {
        "command": "jj.log.rebase",
        "title": "JJ: Rebase Commit"
      },
      {
        "command": "jj.log.squash",
        "title": "JJ: Squash into Current"
      },
      {
        "command": "jj.log.abandon",
        "title": "JJ: Abandon Commit"
      },
      {
        "command": "jj.log.openPatch",
        "title": "JJ: Open Patch"
      },
      {
        "command": "jj.log.bookmark",
        "title": "JJ: Bookmark Actions"
      },
      {
        "command": "jj.log.fetch",
        "title": "JJ: Fetch"
      },
      {
        "command": "jj.log.push",
        "title": "JJ: Push Bookmark"
      },
      {
        "command": "jj.log.refresh",
        "title": "JJ: Refresh Log"
      }
    ],
    "keybindings": [
      {
        "command": "jj.log.open",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j"
      },
      {
        "command": "jj.log.toggleExpand",
        "key": "g e",
        "when": "editorTextFocus && resourceScheme == jj && vim.active && vim.mode == 'normal'"
      },
      {
        "command": "jj.log.edit",
        "key": ["g", "c"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.describe",
        "key": ["g", "d"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.rebase",
        "key": ["g", "r"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.squash",
        "key": ["g", "s"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.toggleExpand",
        "key": ["g", "a"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.openPatch",
        "key": ["="],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.abandon",
        "key": ["g", "A"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.openPatch",
        "key": ["g", "p"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.bookmark",
        "key": ["g", "b"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.fetch",
        "key": ["g", "f"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.push",
        "key": ["g", "P"],
        "when": "editorTextFocus && resourceScheme == jj"
      },
      {
        "command": "jj.log.refresh",
        "key": ["g", "R"],
        "when": "editorTextFocus && resourceScheme == jj"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.28.0",
    "glob": "^8.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3"
  }
}
